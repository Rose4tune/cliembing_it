{
  "name": "cliembing-it",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter=@web dev",
    "dev:sb": "pnpm --filter=@storybook dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter=@web build",
    "build:sb": "pnpm --filter=@storybook build",
    "lint": "turbo run lint",
    "lint:web": "pnpm --filter=@web lint",
    "commitlint": "commitlint",
    "test": "turbo run test",
    "test:web": "pnpm --filter=@web test",
    "typecheck": "turbo run typecheck",
    "typecheck:web": "pnpm --filter=@web typecheck",
    "add:ui": "node scripts/add-shadcn.js",
    "prepare": "husky install",
    "clean": "rimraf dist .next .turbo storybook-static"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.39.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/react": "^10.0.6",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.2",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.4"
  },
  "license": "ISC"
}
