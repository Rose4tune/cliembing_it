{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "storybook-static/**"]
    },
    "dev": { "cache": false, "persistent": true },
    "lint": { "cache": true },
    "test": { "cache": true, "dependsOn": ["^build"] },
    "typecheck": { "cache": true }
  }
}
